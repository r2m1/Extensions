{
  "name": "4com",
  "version": "4.0.0",
  "description": "Integrates with 4Com Live Chat",
  "main": "build/module.js",
  "scripts": {
    "transpile": "tsc -p .",
    "zip": "tar cfz 4com.tar.gz build/* package.json package-lock.json README.md icon.png",
    "build": "npm run transpile && npm run lint && npm run zip",

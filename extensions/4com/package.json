  "dependencies": {
    "@cognigy/extension-tools": "^0.10.0",
    "axios": "^0.21.0",
    "query-string": "^6.13.7",
    "tslint": "^6.1.2",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "^13.13.15",
    "typescript": "^3.8.3"
  }
}
